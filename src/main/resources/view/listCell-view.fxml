<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="100.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="view.ListCellController">
   <HBox maxWidth="1.7976931348623157E308" prefHeight="120.0">
      <VBox maxWidth="1.7976931348623157E308" prefHeight="120.0" HBox.hgrow="ALWAYS">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
         </padding>
         <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
            <Label text="Title:">
               <HBox.margin>
                  <Insets right="5.0"/>
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="14.0"/>
               </font>
            </Label>
            <Text fx:id="titleText" strokeType="OUTSIDE" strokeWidth="0.0">
               <font>
                  <Font size="14.0"/>
               </font>
            </Text>
         </HBox>
         <HBox>
            <Label text="Description:">
               <font>
                  <Font name="System Bold" size="14.0"/>
               </font>
               <HBox.margin>
                  <Insets right="5.0"/>
               </HBox.margin>
            </Label>
            <Text fx:id="descriptionText" strokeType="OUTSIDE" strokeWidth="0.0">
               <font>
                  <Font size="14.0"/>
               </font>
            </Text>
         </HBox>
         <HBox layoutX="20.0" layoutY="20.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
               minWidth="-Infinity">
            <Label text="Cost:">
               <HBox.margin>
                  <Insets right="5.0"/>
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="14.0"/>
               </font>
            </Label>
            <Text fx:id="costText" strokeType="OUTSIDE" strokeWidth="0.0">
               <font>
                  <Font size="14.0"/>
               </font>
            </Text>
         </HBox>
         <HBox layoutX="20.0" layoutY="60.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
               minWidth="-Infinity">
            <Label text="Date:">
               <HBox.margin>
                  <Insets right="5.0"/>
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="14.0"/>
               </font>
            </Label>
            <Text fx:id="dateText" strokeType="OUTSIDE" strokeWidth="0.0">
               <font>
                  <Font size="14.0"/>
               </font>
            </Text>
         </HBox>
         <HBox layoutX="20.0" layoutY="60.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
               minWidth="-Infinity">
            <Label text="Category:">
               <HBox.margin>
                  <Insets right="5.0"/>
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="14.0"/>
               </font>
            </Label>
            <Text fx:id="categoryText" strokeType="OUTSIDE" strokeWidth="0.0">
               <font>
                  <Font size="14.0"/>
               </font>
            </Text>
         </HBox>
      </VBox>
   </HBox>
</AnchorPane>
